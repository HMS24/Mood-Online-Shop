{{#if errorMessage}}
<div class="alert alert-danger" role="alert" style="margin: 30px 0 ;">
  ✘ {{errorMessage}}
</div>
{{/if}}
{{#if errors}}
<ul class="alert alert-danger" role="alert" style="margin: 30px 0;list-style-type:none;">
  {{#each errors}}
  <li>✘ {{this.message}}</li>
  {{/each}}
</ul>
{{/if}}